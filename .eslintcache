     let cafes = snapshot.docs.map((doc) => doc.data());\n\n          setNearbyCafe(cafes);\n        });\n    }\n  }, [state.currentCafe]);\n  return (\n    <NearbyCafeStyle>\n      <div className=\"map-container\">\n        <Map cafeInfo={state.currentCafe} />\n      </div>\n      <ul className=\"cafe-list\">\n        <h3 className=\"cafe-list-title\">주변 카페 추천</h3>\n        {nearbyCafe.map((cafe, index) => (\n          <div key={cafe.id} className=\"cafe-link\">\n            <LinkContent key={index} to={`/content/${cafe.id}`}>\n              <Cover id={cafe.id}></Cover>\n              <CafeList className=\"cafe\">\n                <CafeImage src={cafe.cafeImg} />\n\n                <div className=\"cafe-info\">\n                  <div className=\"cafe-name\">{cafe.cafeName}</div>\n                  <div className=\"cafe-address\">주소 : {cafe.cafeAddress}</div>\n                </div>\n              </CafeList>\n            </LinkContent>\n          </div>\n        ))}\n      </ul>\n    </NearbyCafeStyle>\n  );\n};\n\nfunction mapStateToProps(state, ownProps) {\n  return { ...state };\n}\n\nfunction mapDispatchToProps(dispatch) {\n  return {\n    cafe: (currentCafe) =>\n      dispatch(actionCreators.currentCafeClick(currentCafe)),\n    currentCafeComment: (comment) =>\n      dispatch(actionCreators.currentCafeComment(comment)),\n  };\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(NearbyCafe);\n","/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/ContentHeader/index.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/Map.js",["316","317"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Card/index.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/CommentWrite/index.js",[],["318","319"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/MyCafe/MyCafe.js",["320","321","322","323"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/
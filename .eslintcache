:\n      let cardArr = action.card.slice();\n      return {\n        ...state,\n        cardArr,\n      };\n    case KEYWORD:\n      let keyword = action.keyword;\n      for (let i in state) {\n        if (i === 'keyword') {\n          state[i] = keyword;\n        }\n      }\n      return Object.assign({}, state, {\n        ...state,\n        keyword,\n      });\n    case CURRENTCAFECOMMENT:\n      let comment = action.comment;\n      for (let i in state) {\n        if (i === 'comment') {\n          state[i] = comment;\n        }\n      }\n      return Object.assign({}, state, {\n        ...state,\n        comment,\n      });\n    case CURRENTCAFE:\n      let currentCafe = action.currentCafe;\n      for (let i in state) {\n        if (i === 'currentCafe') {\n          state[i] = currentCafe;\n        }\n      }\n      return (Object.assign({}, state, {\n        ...state,\n        currentCafe,\n      })\n      )\n    case CURRENTUSER:\n      let user = action.user;\n      for (let i in state) {\n        if (i === 'user') {\n          state[i] = user;\n        }\n      }\n      return Object.assign({}, state, {\n        ...state,\n        user,\n      });\n    case CHANGEUSERPROFILE:\n      return {\n        ...state,\n        user: {\n          ...state.user,\n          photoURL: action.profile,\n        },\n      };\n    case CHANGEUSERCOMMENT:\n      if (!state.user.comment) {\n        state.user.comment = [];\n      }\n      return {\n        ...state,\n        user: {\n          ...state.user,\n          comment: action.comment,\n        },\n      };\n    case CHANGEDISPLAYNAME:\n      return {\n        ...state,\n        user: {\n          ...state.user,\n          displayName: action.display,\n        },\n      };\n    case CHANGEUSERHEART:\n      if (!state.user.heart) {\n        state.user.heart = [];\n      }\n\n      return {\n        ...state,\n        user: {\n          ...state.user,\n          heart: action.heart,\n        },\n      };\n    default:\n      return state;\n  }\n};\n\nconst store = createStore(reducer);\n\nexport const actionCreators = {\n  tagNameArray,\n  addCardList,\n  searchKeyword,\n  currentCafeClick,\n  currentCafeComment,\n  currentUser,\n  changeUserProfile,\n  changeUserDisplayName,\n  changeUserComment,\n  changeUserHeart,\n};\n\nexport default store;\n",["149","150"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/getNearbyCafe.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Scope/Scope.js",["151","152","153","154","155","156"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/ContentComment/ContentComment.js",["157","158","159","160","161","162","163","164"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Comment/Comment.js",["165","166"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/CommentWrite/CommentWrite.js",["167","168","169","170","171","172","173","174","175"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/SearchBar/SearchBar.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/CardWrapper/CardWrapper.js",["176","177","178","179","180","181","182","183","184","185","186","187"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Card/Card.js",[],["188","189"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/CardWrapper/Recommendation.js",["190"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/reportWebVitals.js",[],["191","192"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/CardWrapper/index.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/MyCafe/MyCafe.js",["193","194","195","196"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/SearchWrapper/SearchWrapper.js",["197"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Tag/Tag.js",["198"],
return [...pres];\n    });\n  };\n  const handleImageEnlarge = (index) => {\n    setCurrentImg(images[index]);\n    setModal((pres) => !pres);\n  };\n  const handleUnEnlarge = () => {\n    setModal((pres) => !pres);\n  };\n\n  return (\n    <CommentWriteStyle>\n      <UserAndScope>\n        <CommentTitle>카페에 대한 리뷰를 작성해주세요</CommentTitle>\n        <ScopeContainer>\n          <Scope\n            setScope={setScope}\n            modifyScope={beforeModify ? beforeModify.userStar : ''}\n          ></Scope>\n        </ScopeContainer>\n      </UserAndScope>\n\n      <TagWrapper>\n        {commentTags.map((tag) => (\n          <span\n            key={tag}\n            onClick={() => {\n              handleTags(tag);\n            }}\n          >\n            <Tag\n              tagName={tag}\n              isSmall={true}\n              color=\"white\"\n              isButton={true}\n              modifyTag={beforeModify ? beforeModify.userTag : ''}\n            ></Tag>\n          </span>\n        ))}\n      </TagWrapper>\n      <CommentInput\n        onChange={(e) => {\n          setSubmitComment(e.target.value);\n        }}\n      >\n        {beforeModify ? beforeModify.userComment : ''}\n      </CommentInput>\n      <CommentImgWrapper>\n        <UploadImg>\n          <UploadImgInput\n            type=\"file\"\n            onChange={(e) => {\n              if (e.target.files[0]) {\n                upLoadTaskHandler(e.target.files[0]);\n              }\n            }}\n          ></UploadImgInput>\n        </UploadImg>\n\n        {images.map((image, index) => {\n          return (\n            <Uploaded key={index}>\n              <UploadedImgCover key={index}>\n                <RemoveImg\n                  data-index={index}\n                  onClick={(e) => {\n                    handleImageRemove(e.target.dataset.index);\n                  }}\n                  src={removeImg}\n                ></RemoveImg>\n                <EnlargeImg\n                  data-index={index
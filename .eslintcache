ents/utils/Scope/Scope.js",["145","146","147","148","149","150"],"import { useEffect, useLayoutEffect, useMemo, useState } from 'react';\nimport { Redirect } from 'react-router-dom';\nimport styled from 'styled-components';\n\nconst Polygon = styled.polygon`\n  fill: ${(props) => props.color};\n  transition: 0.4s;\n  border: 1px solid black;\n`;\n\nconst ScopeStyle = styled.button`\n  background-color: inherit;\n  outline: none;\n  border: initial;\n  display: inherit;\n`;\n\nconst Star = (props) => {\n  let index = props.index;\n  return (\n    <svg\n      onClick={props.click}\n      onMouseOver={props.onhover}\n      onMouseLeave={props.unhover}\n      xmlns=\"http://www.w3.org/2000/svg\"\n      width={props.size}\n      height={props.size}\n      viewBox=\"0 0 12 12\"\n    >\n      <Polygon\n        color={props.color}\n        points=\"6 8.5200001 2.47328849 10.854102 3.60333748 6.77872286 0.293660902 4.14589803 4.51878111 3.96127709 6 0 7.48121889 3.96127709 11.7063391 4.14589803 8.39666252 6.77872286 9.52671151 10.854102\"\n      ></Polygon>\n    </svg>\n  );\n};\n\nconst Scope = (props) => {\n  const propsSetScope = props.setScope ? props.setScope : () => {};\n  const [isHovering, setHover] = useState(-1);\n  let size = '21px';\n  const [scope, setScope] = useState(-1);\n  const [stars, setStars] = useState([]);\n  const [fixedStars, setFixedStars] = useState([]);\n  // 변하는 scope\n  useMemo(() => {\n    let tempStar = [];\n    for (let i = 0; i <= isHovering; i++) {\n      tempStar.push(\n        <Star\n          onhover={() => {\n            setHover(i);\n          }}\n          unhover={() => {\n            setHover(-1);\n          }}\n          click={() => {\n            setScope(i);\n            propsSetScope(i + 1);\n          }}\n          key={i}\n          index={i}\n          size={size}\n          color=\"#f5b342\"\n        ></Star>\n      );\n    }\n    for (let i = isHovering + 1; i < 5; i++) {\n      tempStar.push(\n        <Star\n          onhover={() => setHover(i)}\n          unhover={() => setHover(-1)}\n          color=\"#dddddd\"\n          key={i}\n          index={i}\n          size={size}\n        ></Star>\n      );\n    }\n    setStars(tempStar);\n  }, [isHovering, scope]);\n\n  // 고정된 scope\n  useMemo(() => {\n    let tempStars = [];\n    for (let i = 0; i <= scope; i++) {\n      tempStars.push(\n        <Star\n          click={() => {\n            if (scope === -1) {\n              setScope(i);\n            }\n          }}\n          key={i}\n          index={i}\n          size={size}\n          color=\"#f59642\"\n        ></Star>\n      );\n    }\n    for (let i = scope + 1; i < 5; i++) {\n      tempStars.push(\n        <Star color=\"#dddddd\" key={i} index={i} size={size}></Star>\n      );\n    }\n    setFixedStars(tempStars);\n  }, [scope]);\n\n  if (props.isScope) {\n    let PropsScope = props.scope;\n    if ((PropsScope > 5) | (PropsScope < 0)) {\n      PropsScope = 0;\n    }\n    let contentsScope = [];\n    for (let i = 0; i <= PropsScope - 1; i++) {\n      contentsScope.push(\n        <Star key={i} index={i} size={props.size} color=\"#f59642\"></Star>\n      );\n    }\n    for (let i = PropsScope; i < 5; i++) {\n      contentsScope.push(\n        <Star color=\"#dddddd\" key={i} index={i} size={props.size}></Star>\n      );\n    }\n    return <span>{contentsScope}</span>;\n  }\n  if (scope !== -1) {\n    return (\n      <ScopeStyle\n        onClick={() => {\n          setScope(-1);\n          propsSetScope(-1);\n        }}\n      >\n        {fixedStars}\n      </ScopeStyle>\n    );\n  } else {\n    return <ScopeStyle>{stars}</ScopeStyle>;\n  }\n};\n\nexport default Scope;\n","/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/CommentWrite/CommentWrite.js",["151","152","153","154","155","156"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/Nav/Nav.js",["157"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/Signin/aut
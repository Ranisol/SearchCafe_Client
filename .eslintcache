\n  useEffect(() => {\n    if (props.modifyScope) {\n      setScope(props.modifyScope - 1);\n    }\n  }, []);\n  // 변하는 scope\n  useMemo(() => {\n    let tempStar = [];\n    for (let i = 0; i <= isHovering; i++) {\n      tempStar.push(\n        <Star\n          onhover={() => {\n            setHover(i);\n          }}\n          unhover={() => {\n            setHover(-1);\n          }}\n          click={() => {\n            setScope(i);\n            propsSetScope(i + 1);\n          }}\n          key={i}\n          index={i}\n          size={size}\n          color=\"#f5b342\"\n        ></Star>\n      );\n    }\n    for (let i = isHovering + 1; i < 5; i++) {\n      tempStar.push(\n        <Star\n          onhover={() => setHover(i)}\n          unhover={() => setHover(-1)}\n          color=\"#dddddd\"\n          key={i}\n          index={i}\n          size={size}\n        ></Star>\n      );\n    }\n    setStars(tempStar);\n  }, [isHovering, scope]);\n\n  // 고정된 scope\n  useMemo(() => {\n      let tempStars = [];\n      for (let i = 0; i <= scope; i++) {\n        tempStars.push(\n          <Star\n            click={() => {\n              if (scope === -1) {\n                setScope(i);\n              }\n            }}\n            key={i}\n            index={i}\n            size={size}\n            color=\"#f59642\"\n          ></Star>\n        );\n      }\n      for (let i = scope + 1; i < 5; i++) {\n        tempStars.push(\n          <Star color=\"#dddddd\" key={i} index={i} size={size}></Star>\n        );\n      }\n      setFixedStars(tempStars);\n  }, [scope]);\n\n  if (props.isScope) {\n    let PropsScope = props.scope;\n    if ((PropsScope > 5) | (PropsScope < 0)) {\n      PropsScope = 0;\n    }\n    let contentsScope = [];\n    for (let i = 0; i <= PropsScope - 1; i++) {\n      contentsScope.push(\n        <Star key={i} index={i} size={props.size} color=\"#f59642\"></Star>\n      );\n    }\n    for (let i = PropsScope; i < 5; i++) {\n      contentsScope.push(\n  
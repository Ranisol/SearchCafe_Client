color=\"#ffffff\"\n            tagName={tagnames.이디야}\n          ></Tag>\n          <Tag\n            isButton={true}\n            handleTags={handleTags}\n            color=\"#ffffff\"\n            tagName={tagnames.커피빈}\n          ></Tag>\n          <Tag\n            isButton={true}\n            handleTags={handleTags}\n            color=\"#ffffff\"\n            tagName={tagnames.탐앤탐스}\n          ></Tag>\n          <Tag\n            isButton={true}\n            handleTags={handleTags}\n            color=\"#ffffff\"\n            tagName={tagnames.투썸플레이스}\n          ></Tag>\n          <Tag\n            isButton={true}\n            handleTags={handleTags}\n            color=\"#ffffff\"\n            tagName={tagnames.폴바셋}\n          ></Tag>\n          <Tag\n            isButton={true}\n            handleTags={handleTags}\n            color=\"#ffffff\"\n            tagName={tagnames.할리스}\n          ></Tag>\n        </TagWrapperStyleDiv>\n      </TagWrapperStyle>\n      <WrapperTitle>찾는 카페가 있으신가요?</WrapperTitle>\n      <SearchBarWrapperStyle>\n        <SearchBar setKeyword={setKeyword}></SearchBar>\n      </SearchBarWrapperStyle>\n    </SearchWrapperStyle>\n  );\n};\n\nfunction mapStateToProps(state, ownProps) {\n  return { state };\n}\n\nfunction mapDispatchToProps(dispatch) {\n  return {\n    tagNameArray: (tagName) => {\n      dispatch(actionCreators.tagNameArray(tagName));\n    },\n    searchKeyword: (keyword) => {\n      dispatch(actionCreators.searchKeyword(keyword));\n    },\n  };\n}\nexport default connect(mapStateToProps, mapDispatchToProps)(SearchWrapper);\n","/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Tag/Tag.js",["206"],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Card/index.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/Tag/tagnames.js",[],"/home/ranisol/Desktop/projectfolder/SearchCaffe_client/src/components/utils/ImageModal/ImageModal.js",[],"/home/raniso